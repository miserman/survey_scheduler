{
  "name": "survey_scheduler",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "sh tools/use_swc.sh && next dev",
    "build": "sh tools/use_swc.sh && next build",
    "lint": "next lint",
    "cypress:open": "cypress open",
    "test-serve": "sh tools/use_babel.sh && next dev",
    "test": "cypress run"
  },
  "nyc": {
    "report-dir": "coverage",
    "reporter": [
      "html"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-cognito-identity-provider": "latest",
    "@aws-sdk/client-dynamodb": "latest",
    "@aws-sdk/client-sns": "latest",
    "@aws-sdk/lib-dynamodb": "latest",
    "@emotion/react": "latest",
    "@emotion/styled": "latest",
    "@mui/icons-material": "latest",
    "@mui/material": "latest",
    "aws-jwt-verify": "latest",
    "next": "latest",
    "react": "latest"
  },
  "devDependencies": {
    "@cypress/code-coverage": "latest",
    "@jest/globals": "latest",
    "@types/jsonwebtoken": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "babel-plugin-istanbul": "latest",
    "cypress": "latest",
    "eslint": "latest",
    "typescript": "latest"
  }
}
